{
  "version": "1.0.0",
  "base_path": "~/local-deployer",
  "docker_network_prefix": "ldm",
  "default_stack": "laravel-vue",
  "default_db": "mysql",
  "default_ports": {
    "http": 80,
    "https": 443,
    "mysql": 3306,
    "postgres": 5432
  },
  "auto_backup_on_deploy": false,
  "max_backups_per_project": 10,
  "log_level": "INFO",
  "java_version": "21",
  "php_version": "8.2",
  "node_version": "20"
}
